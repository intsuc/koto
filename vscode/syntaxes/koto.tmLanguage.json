{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "ヿ",
  "scopeName": "source.koto",
  "repository": {},
  "patterns": [
    {
      "match": "#.*$",
      "name": "comment.line.koto"
    },
    {
      "match": "\\b(?:fun)\\b|[:→]",
      "name": "keyword.koto"
    },
    {
      "match": "\\b(?:type|int64)\\b",
      "name": "constant.language.koto"
    },
    {
      "match": "-?[0-9]+\\b",
      "name": "constant.numeric.koto"
    },
    {
      "match": "\\b[a-z0-9-]+\\b",
      "name": "variable.koto"
    }
  ]
}
