let x1 = {}
let x2 : {} = {}
let x3 : type = {}
let x4 = { a = 1 }
let x5 : { a = int64 } = { a = 1 }
let x6 = { a = 1, b = true }

# type-directed field ordering
let x7 : { a = int64, b = bool } = { a = 1, b = true }
let x8 : { a = int64, b = bool } = { b = true, a = 1 }

# no subtyping
# let - : { a = int64 } = { a = 1, b = true }

let x9 = { a = 1 }.a
let x10 = { a = { b = 2 } }.a.b

type
